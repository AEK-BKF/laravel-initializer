{
    "name": "mad-web/laravel-initializer",
    "description": "Package provider ability to setup pre/post install/update actions",
    "type": "library",
    "keywords": [
        "mad-web",
        "laravel-initializer"
    ],
    "homepage": "https://github.com/mad-web/laravel-initializer",
    "license": "MIT",
    "authors": [
        {
            "name": "Mad Web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.4",
        "illuminate/container": "^5.4",
        "illuminate/console": "^5.4",
        "illuminate/config": "^5.4",
        "illuminate/bus": "^5.4",
        "symfony/process": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "ZFort\\AppInstaller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFort\\AppInstaller\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZFort\\AppInstaller\\InstallerServiceProvider"
            ]
        }
    }
}
